{
  "name": "quickmaker-ui",
  "version": "1.0.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:web\" \"npm:dev:electron:build\" \"npm:dev:electron:run\"",
    "dev:web": "vite",
    "dev:electron:build": "tsc -p electron/tsconfig.json --watch",
    "dev:electron:run": "wait-on dist-electron/main.js http://localhost:5173 && nodemon --watch dist-electron --ext js --exec \"electron dist-electron/main.js --dev\"",
    "build": "npm run build:web && npm run build:electron",
    "build:web": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json"
  },
  "dependencies": {
    "element-plus": "^2.13.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.1.2",
    "electron": "^35.0.1",
    "nodemon": "^3.1.9",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.2"
  }
}
